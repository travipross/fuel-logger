{
    "plugins": [
        "@semantic-release/commit-analyzer", 
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github", {
                "assets": [
                    {"path": "dist/**"}
                ]
            }
        ],
        [
            "@semantic-release/exec", {
                "publishCmd": "export VERSION=${nextRelease.version}; make python build-docker publish-docker && echo \"VERSION=${VERSION}\" >> $GITHUB_ENV"
            }
        ]
    ]
}